{
  "name": "proshop",
  "version": "1.0.0",
  "description": "eCommerce application build with the mern stack",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon run backend/server.js",
