    "bootstrap": "^5.3.3",
    "colors": "^1.4.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.1",
    "react-bootstrap": "^2.10.4"
  }
}
